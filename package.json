{
  "name": "@jcgalvis/vtex.livestreaming",
  "version": "1.2.3",
  "description": "Made with create-react-library",
  "author": "JCGALVIS",
  "license": "MIT",
  "repository": "https://github.com/JCGALVIS/vtex.livestreaming.git",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --define GET_LIVESTREAMING_CONFIG_URL='__GET_LIVESTREAMING_CONFIG_URL',GET_CHAT_BY_ID_URL='__GET_CHAT_BY_ID_URL' --no-compress --format modern,cjs",
    "start": "microbundle-crl --define GET_LIVESTREAMING_CONFIG_URL='https://x5vzeovx68.execute-api.us-east-1.amazonaws.com/Prod/livestreamingconfig',GET_CHAT_BY_ID_URL='https://x5vzeovx68.execute-api.us-east-1.amazonaws.com/Prod/chat' watch --no-compress --format modern,cjs",
    "start:prod": "microbundle-crl --define GET_LIVESTREAMING_CONFIG_URL='https://mtumok1rr4.execute-api.us-east-1.amazonaws.com/Prod/livestreamingconfig',GET_CHAT_BY_ID_URL='https://mtumok1rr4.execute-api.us-east-1.amazonaws.com/Prod/chat' watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/resize-observer-browser": "^0.1.6",
    "@vtex/css-handles": "^1.1.3",
    "@vtex/styleguide": "^9.143.0",
    "react-transition-group": "^4.4.2",
    "uuid": "^8.3.2",
    "vtex-tachyons": "^3.2.2"
  }
