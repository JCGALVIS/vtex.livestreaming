{
  "name": "@jcgalvis/vtex.livestreaming",
  "version": "1.7.0",
  "description": "Made with create-react-library",
  "author": "JCGALVIS",
  "license": "MIT",
  "repository": "https://github.com/JCGALVIS/vtex.livestreaming.git",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "start:prod": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\""
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@types/lodash.clonedeep": "^4.5.6",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^3.2.3",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "typescript": "^4.5.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@giphy/js-types": "^4.1.0",
    "@types/canvas-confetti": "^1.4.2",
    "@types/resize-observer-browser": "^0.1.6",
    "@lottiefiles/react-lottie-player": "^3.4.2",
    "@vtex/css-handles": "^1.1.3",
    "@vtex/styleguide": "^9.143.0",
    "caniuse-lite": "^1.0.30001312",
    "canvas-confetti": "^1.5.1",
    "gsap": "^3.9.1",
    "kuikpay-sdk": "0.0.85-dev.1",
    "react-giphy-searchbox": "^1.5.4",
    "react-intl": "^5.21.2",
    "react-transition-group": "^4.4.2",
    "tinycolor": "^0.0.1",
    "uuid": "^8.3.2",
    "vtex-tachyons": "^3.2.2",
    "lodash.clonedeep": "^4.5.0"
  }
}
